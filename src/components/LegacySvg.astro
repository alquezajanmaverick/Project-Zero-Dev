<script>
  const animate = () => {
    anime({
      targets: "#svg2",
      opacity: [0, 1],
      easing: "easeInOutExpo",
      duration: 1000,
    });
    anime({
      targets: ".star",
      translateY: [-250, 0],
      opacity: [0, 1],
      scale: [1, 1.05],
      easing: "easeInOutExpo",
      duration: 2000,
    });
    let anim = anime({
      targets: ".yellow_star",
      opacity: [0, 1],
      easing: "easeInOutExpo",
      duration: 3000,
    });
    anim.finished.then(function () {
      if (
        window.location.pathname == "/legacy" ||
        window.location.pathname == "/legacy/"
      ) {
        setTimeout(() => {
          window.swup.loadPage({
            url: "/legacy/1", // URL of request (defaults to current url)
          });
        }, 2500);
      }
    });
  };
  document.addEventListener("swup:animationInDone", () => {
    animate();
  });
  document.addEventListener("DOMContentLoaded", () => {
    animate();
  });
</script>
